}

@using (Html.BeginForm("CommonCustomer", "Configuration", FormMethod.Post, new { id = "commonCustomerFormId", @class = "formValidator", @enctype = "multipart/form-data" }))
{
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    @Html.HiddenFor(model => model.ID, new { Value = "", id = "customerIdDelete" })
    @Html.HiddenFor(model => model.ActionId, new { Value = "", id = "ActionIdDelete" })
    @Html.HiddenFor(model => model.CompanyFK, new { id = "CompanyFKDelete" })

    <div class="modal fade" id="deleteModal" data-toggle="modal" data-backdrop="static" data-keyboard="false" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close pull-right" data-dismiss="modal" aria-label="Close">
                        &times;
                    </button>
                    <h5 class="modal-title"><b>Customer Delete</b></h5>

                </div>
                <div class="modal-body">
                    <div style="text-align:center">
                        <h3>Are you sure to delete this customer?</h3>
                    </div>
                </div>

                <div class="modal-footer">
                    <button type="button" id="btnClose" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" id="btnDelete" class="deleteId btn btn-primary"><i class="glyphicon glyphicon-check"></i>Confirm</button>
                </div>
            </div>
        </div>
    </div>
}


@section scripts {

    <script type="text/javascript">

        let areaValue = 0;
        $(document).ready(function () {
            $('.formValidator').bootstrapValidator();
            $("#ddlCommon_DivisionFk").change(function (e) {
                districtChange(0);
            });

        });
        $("#ddlCommon_DistrictsFk").change(function (e) {
            var commonDistrictsFk = $("#ddlCommon_DistrictsFk :selected").val();

            upazilaChange(0, commonDistrictsFk);
        });
        $('#ddlZoneId').on('change', function () {
            var zoneId = $("#ddlZoneId :selected").val();

            zoneDivisionChange(0, zoneId);

        });

        $("#ddlZoneDivisionId").on('change', function () {
            var zoneId = $("#ddlZoneId :selected").val();
            var zoneDivisionId = $("#ddlZoneDivisionId :selected").val();
            regionChange(0, zoneId, zoneDivisionId);


        });
        $("#ddlRegionId").on("change", function () {
            var zoneId = $("#ddlZoneId :selected").val();
            var zoneDivisionId = $("#ddlZoneDivisionId :selected").val();
            var regionId = $("#ddlRegionId :selected").val();
            areaChange(0, zoneId, zoneDivisionId, regionId);
        });
        $("#ddlAreaId").on("change", function () {
            var zoneId = $("#ddlZoneId :selected").val();
            var zoneDivisionId = $("#ddlZoneDivisionId :selected").val();
            var regionId = $("#ddlRegionId :selected").val();
            var areaId = $("#ddlRegionId :selected").val();
            territoryChange(0, zoneId, zoneDivisionId, regionId, areaId);
        });


        function zoneDivisionChange(zoneDivisionValue,zoneValue) {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetZoneDivisionList", "Configuration")",
                datatype: "Json",
                data: { companyId:@Model.CompanyFK, zoneId: zoneValue },
                success: function (data) {

                    if (data) {
                        $("#ddlZoneDivisionId").empty();
                        //$("#ddlZoneDivisionId").append("<option value=''>==Select Division==</option>");
                        $(data).each(function (index, item) {
                            $("#ddlZoneDivisionId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                        });
                        if (zoneDivisionValue > 0) {
                            $("#ddlZoneDivisionId").val(zoneDivisionValue);
                        }

                    }
                }
            });
        }



        function regionChange(regionValue, zoneValue, zoneDivisionValue) {

            $.ajax({
                type: "GET",
                url: "@Url.Action("GetRegionList", "Configuration")",
                datatype: "Json",
                data: { companyId:@Model.CompanyFK, zoneId: zoneValue, zoneDivisionId: zoneDivisionValue },
                success: function (data) {

                    if (data) {
                        $("#ddlRegionId").empty();
                        //$("#ddlRegionId").append("<option value=''>==Select Region==</option>");
                        $(data).each(function (index, item) {
                            $("#ddlRegionId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                        });
                        if (regionValue > 0) {
                            $("#ddlRegionId").val(regionValue);

                        }
                    }
                }
            });
        }

        function areaChange(areaValue, zoneValue, zoneDivisionValue, regionValue) {

            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAreaList", "Configuration")",
                datatype: "Json",
                data: { companyId:@Model.CompanyFK, zoneId: zoneValue, zoneDivisionId: zoneDivisionValue, regionId: regionValue },
                success: function (data) {
                    if (data) {
                        $("#ddlAreaId").empty();
                        $(data).each(function (index, item) {
                            $("#ddlAreaId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                        });
                        if (areaValue > 0) {
                            $("#ddlAreaId").val(areaValue);
                        }

                    }
                }
            });

        }

        function territoryChange(territoryValue, zoneValue, zoneDivisionValue, regionValue, areaValue) {

            //var zoneId = $("#ddlZoneId :selected").val();
             $.ajax({
                type: "GET",
                url: "@Url.Action("GetSubZoneList", "Configuration")",
                datatype: "Json",
                 data: { companyId:@Model.CompanyFK, zoneId: zoneValue, zoneDivisionId: zoneDivisionValue, regionId: regionValue,areaId:areaValue },
                success: function (data) {
                    if (data) {
                        $("#ddlSubZoneId").empty();
                        //$("#ddlSubZoneId").append("<option value=''>==Select Territory==</option>");
                        $(data).each(function (index, item) {
                            $("#ddlSubZoneId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                            console.log(item.Text);
                        });
                        if (territoryValue > 0) {
                            $("#ddlSubZoneId").val(territoryValue);
                        }
                    }
                }
            });
        }
        function districtChange(districtValue) {
             var commonDivisionFk = $("#ddlCommon_DivisionFk :selected").val();
                $.ajax({
                type: "GET",
                url: "@Url.Action("CommonDistrictsGet","Configuration")",
                datatype: "Json",
                data: { id:  commonDivisionFk },
                    success: function (data) {

                        if (data) {
                            $("#ddlCommon_DistrictsFk").empty();
                            $("#ddlCommon_DistrictsFk").append("<option value=''>==Select District==</option>");
                            $(data).each(function (index, item) {
                                $("#ddlCommon_DistrictsFk").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                            });
                            if (districtValue > 0) {
                                $("#ddlCommon_DistrictsFk").val(districtValue);
                            }
                        }
                    }
                });
        }



        function upazilaChange(upazilaValue,districtValue) {
                $.ajax({
                type: "GET",
                url: "@Url.Action("CommonUpazilasGet", "Configuration")",
                datatype: "Json",
                    data: { id: districtValue },
                    success: function (data) {
                        if (data) {
                            $("#ddlCommon_UpazilasFk").empty();
                            $("#ddlCommon_UpazilasFk").append("<option value=''>==Select Police Station==</option>");
                            $(data).each(function (index, item) {
                                $("#ddlCommon_UpazilasFk").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                            });
                            if (upazilaValue > 0)
                            {
                                $("#ddlCommon_UpazilasFk").val(upazilaValue);
                            }
                        }
                    }
                });
        }



        function makeEditble(id) {
            if (id !== null) {
                $.ajax({
                type: "get",
                url: "@Url.Action("CommonCustomerByIDGet", "Configuration")",
                    data: { id: id },
                    datatype: "json",
                    traditional: true,
                    success: function (data) {

                        $('#btnSave').html("<i class='fa fa-edit'></i>&nbsp;Update");
                        $('#customerId').val(data.ID);
                        $('#ActionId').val(2);
                        $('#CompanyFK').val(data.CompanyFK);
                        $('#txtCustomerName').val(data.Name);
                        $('#txtContactPerson').val(data.ContactPerson);
                        $('#txtPhone').val(data.Phone);


                        $('#txtEmail').val(data.Email);
                        //$('#ddlCommon_DivisionFk').val(data.Common_DivisionFk).trigger('change');
                        //districtChange(data.Common_DistrictsFk);
                        //upazilaChange(data.Common_UpazilasFk, data.Common_DistrictsFk);

                        //$('#ddlCommon_DistrictsFk').val(data.Common_DistrictsFk).trigger('change');
                        //$('#ddlCommon_UpazilasFk').val(data.Common_UpazilasFk).trigger('change');
                        $('#ddlZoneId').val(data.ZoneId).trigger('change');

                        zoneDivisionChange(data.ZoneDivisionId, data.ZoneId);
                        /* $('#ddlZoneDivisionId').val(data.ZoneDivisionId).trigger('change');*/

                        regionChange(data.RegionId, data.ZoneId, data.ZoneDivisionId);
                        areaChange(data.AreaId, data.ZoneId, data.ZoneDivisionId, data.RegionId);

