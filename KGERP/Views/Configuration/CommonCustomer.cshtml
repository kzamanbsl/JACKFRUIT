                                                <td>
                                                    @item.Email
                                                </td>
                                                <td>
                                                    @item.NID
                                                </td>
                                                <td>
                                                    @item.SecurityAmount
                                                </td>

                                                <td>
                                                    @item.CreditLimit
                                                </td>
